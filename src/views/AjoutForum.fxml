<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="703.0" prefWidth="1068.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlleur.AjoutForumController">
   <children>
      <TableView fx:id="tableforum" layoutX="38.0" layoutY="60.0" prefHeight="432.0" prefWidth="876.0">
        <columns>
          <TableColumn fx:id="ColumnForumNom" onEditCommit="#cellNomEdit" prefWidth="113.0" text="Nom" />
          <TableColumn fx:id="ColumnForumDescription" onEditCommit="#cellDescEdit" prefWidth="762.0" text="Description" />
        </columns>
      </TableView>
      <TextField fx:id="fieldnom" layoutX="135.0" layoutY="517.0" />
      <TextArea fx:id="fielddes" layoutX="476.0" layoutY="523.0" prefHeight="131.0" prefWidth="197.0" />
      <Label layoutX="77.0" layoutY="522.0" text="Nom" />
      <Label layoutX="366.0" layoutY="522.0" text="Description" />
      <Button fx:id="ajout" layoutX="712.0" layoutY="533.0" mnemonicParsing="false" onAction="#ajoutForum" text="Ajouter" />
      <Button fx:id="delet" layoutX="945.0" layoutY="100.0" mnemonicParsing="false" text="Supprimer" />
      <TextField fx:id="searchfield" layoutX="779.0" layoutY="14.0" onKeyTyped="#searchForum" />
